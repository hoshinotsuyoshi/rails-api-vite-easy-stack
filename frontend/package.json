{
  "name": "my-spa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "graphql-codegen": "graphql-codegen",
    "lint": "npm run lint:code && npm run lint:style && npm run lint:check",
    "lint:code": "biome lint ./",
    "lint:style": "biome format ./",
    "lint:check": "biome check ./",
    "fix": "npm run fix:code && npm run fix:style && npm run fix:check",
    "fix:code": "biome lint --write ./",
    "fix:style": "biome format --write ./",
    "fix:check": "biome check --write ./"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@pandacss/dev": "^0.46.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^15.9.0",
    "graphql": "^16.9.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
